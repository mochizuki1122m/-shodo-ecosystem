# Shodo Ecosystem Environment Variables
# Copy to .env and update values

# Basic
APP_NAME=Shodo Ecosystem
ENVIRONMENT=development
SERVICE_NAME=shodo-ecosystem

# Database (postgresql+asyncpg://)
DATABASE_URL=postgresql+asyncpg://shodo:shodo_pass@postgres:5432/shodo
REDIS_URL=redis://redis:6379

# AI Server (no /v1 suffix)
VLLM_URL=http://vllm:8001

# Security (CHANGE ALL!)
SECRET_KEY=change-in-production
JWT_SECRET_KEY=change-in-production
AUDIT_SIGNING_KEY=change-in-production

# Frontend
REACT_APP_API_URL=http://localhost
